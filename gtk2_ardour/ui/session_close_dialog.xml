        x="220" 
        y="90" 
        width="80" 
        height="22"
        winfont ="Arial Bold 10"
        macfont ="Helvetica Bold 10"
        fgnormal="#6D6E72"
    	bgnormal="#CACAC5"
        fgactive="#EDECE8"
    	bgactive="#6D6E72"
    	borderwidth="0 0 0 0"
        bordercolor="#6D6E72"/>
        
    <Button style="generic_button"
        id="save_button"
        text="Save"
        x="310" 
        y="90" 
        width="80" 
        height="22"
        winfont ="Arial Bold 10"
        macfont ="Helvetica Bold 10"
        fgnormal="#6D6E72"
    	bgnormal="#CACAC5"
        fgactive="#EDECE8"
    	bgactive="#6D6E72"
    	borderwidth="0 0 0 0"
        bordercolor="#6D6E72"/>
        
    </Layout>
